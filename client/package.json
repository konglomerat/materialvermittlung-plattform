    "underscore": "^1.12.0"
  },
  "scripts": {
    "theme:watch": "node ./src/styles/compile.js --watch",
    "theme:build": "node ./src/styles/compile.js",
    "mailtemplates:build": "node buildMailTemplates.js",
    "mailtemplates:watch": "node buildMailTemplates.js --watch",
    "start": "react-scripts start",
    "build": "npm run mailtemplates:build && npm run theme:build && react-scripts build",
    "graphql": "npm run graphql:schema && npm run graphql:generate",
    "graphql:schema": "docker-compose exec api bin/console api:graphql:export > ./schema.graphql",
    "graphql:generate": "rm -rf src/graphql/generated && apollo codegen:generate --localSchemaFile schema.graphql --target typescript --addTypename --includes 'src/graphql/**/*.ts' --outputFlat src/graphql/generated"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.1",
    "@graphql-codegen/typescript": "^1.17.11",
