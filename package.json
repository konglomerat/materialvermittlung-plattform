{
  "dependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "admin/src/**/*": [
      "prettier --write"
    ],
    "client/src/**/*!(.ttf)": [
      "prettier --write"
    ]
  }
}
